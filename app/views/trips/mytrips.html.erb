<%@trips.each do |trip|%>
  <div class ="row">
    <h1 href="/trips/<%= trip.id %>"><%=trip.name%></h1>
      <div class ="col-sm-4">
      <% trip.trippings.each do |tripping| %>
          <a href="/walks/<%= tripping.walk_id %>"><%=tripping.walk.title%></a>
      <% end %>
      </div>
  </div>
<% end %>